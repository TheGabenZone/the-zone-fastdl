"VertexlitGeneric"
{
	"$basetexture" "vgui\replay\thumbnails\models/workshop/weapons/c_models/c_csgo_awp/classic_force_glass_01"
	
	"$teamcolors" "1"	
	
	"$basemapalphaphongmask" "1"
	"$basealphaenvmapmask" "1"

	"$envmap" "env_cubemap"
	"$envmaptint" "[0.3 0.3 0.3]"	

	"$phong" "1"
	"$phongexponent" "7"
	"$phongboost" "0.2"	
	"$lightwarptexture" "models\lightwarps\weapon_lightwarp"
	"$phongfresnelranges"	"[1 .3 1]"


"$glowcolor" "1"


"$tempvar" "0"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps/cubemap_sheen001"
	"$sheenmapmask" 		"Effects/AnimatedSheen/animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	
	
		"WeaponSkin" {
		}
	}
}