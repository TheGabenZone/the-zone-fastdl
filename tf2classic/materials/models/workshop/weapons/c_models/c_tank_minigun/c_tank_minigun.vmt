"VertexlitGeneric"
{
	"$basetexture" "models/workshop/weapons/c_models/c_tank_minigun/c_tank_minigun"
	"$bumpmap" "models/workshop/weapons/c_models/c_tank_minigun/c_tank_minigun_bumpmap"
	"$selfillummask" "models/workshop/weapons/c_models/c_tank_minigun/c_tank_minigun_illum"
	"$phong" "1"
	"$phongexponent" "15"
	"$phongboost" "1.7"
	"$lightwarptexture" "models/lightwarps/weapon_lightwarp"
	"$phongfresnelranges"	"[.5 .5 3]"
	"$halflambert" "1"

	"$selfillum" "1"
	"$selfillumtint" "[400 400 400]"

	// Rim lighting parameters
	"$rimlight" "1"			
	"$rimlightexponent" "6"	
	"$rimlightboost" ".5"

	"$glowcolor" "1"


	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps/cubemap_sheen001"
	"$sheenmapmask" 		"Effects/AnimatedSheen/animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
		"Sine"
		{
			"sineperiod" "3"
			"sinemin" 0
			"sinemax" 0.5
			"resultVar" "$selfillumtint"
		}
		"Sine"
		{
			"sineperiod" "3"
			"sinemin" 0.2
			"sinemax" 1.5
			"resultVar" "$selfillumtint"
		}
		"Sine"
		{
			"sineperiod" "5"
			"sinemin" 1
			"sinemax" 3
			"resultVar" "$selfillumtint"

		}
	
	}
}































